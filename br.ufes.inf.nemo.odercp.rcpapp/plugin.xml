<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="br.ufes.inf.nemo.odercp.rcpapp.Application">
         </run>
      </application>
   </extension>
  
   <extension id="product" point="org.eclipse.core.runtime.products">
  <product application="br.ufes.inf.nemo.odercp.rcpapp.application" name="ODE RCP">
    <property name="appName" value="ODE RCP"> </property>
    <property name="applicationXMI" value="br.ufes.inf.nemo.odercp.rcpapp/Application.e4xmi"> </property>
  </product>
</extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="OptionMenu"
               label="Options">
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.ChangePassword"
                  label="Change Password"
                  style="push">
            </command>
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.exit"
                  label="Exit"
                  mnemonic="CTRL+W"
                  style="push">
            </command>
         </menu>
         <menu
               label="Project">
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateProject"
                  label="Create Project"
                  style="push">
            </command>
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.DeleteProject"
                  label="Delete Project"
                  style="push">
            </command>
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.SelectProject"
                  label="Select Project"
                  style="push">
            </command>
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.DefineTeam"
                  label="Define Team"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="br.ufes.inf.nemo.odercp.rcpapp.sourceprovider.active">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
         <menu
               label="Administration">
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateUser"
                  label="Create User"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="br.ufes.inf.nemo.odercp.rcpapp.sourceprovider.active">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
         <menu
               label="Resources">
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateHumanResource"
                  label="Create Human Resource"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="br.ufes.inf.nemo.odercp.rcpapp.sourceprovider.active">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
         <menu
               label="Knowledge">
            <menu
                  label="Resources">
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKHumanResource"
                     label="Create Human Resource"
                     style="push">
               </command>
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKHardwareResource"
                     label="Create Hardware Resource"
                     style="push">
               </command>
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKSoftwareResource"
                     label="Create Software Resource"
                     style="push">
               </command>
            </menu>
            <menu
                  label="Processes">
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKActivity"
                     label="Create Activity"
                     style="push">
               </command>
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKArtefact"
                     label="Create Artefact"
                     style="push">
               </command>
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKLifeCycleModel"
                     label="Create Life-Cycle Model"
                     style="push">
               </command>
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKProcedure"
                     label="Create Procedure"
                     style="push">
               </command>
               <command
                     commandId="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKProcess"
                     label="Create Process"
                     style="push">
               </command>
            </menu>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="br.ufes.inf.nemo.odercp.rcpapp.sourceprovider.active">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
         <menu
               label="Tools">
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.ProcessDefinition"
                  label="Process Definition"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="br.ufes.inf.nemo.odercp.rcpapp.sourceprovider.active">
                  <equals
                        value="ENABLED">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
         <menu
               label="Help">
            <command
                  commandId="br.ufes.inf.nemo.odercp.rcpapp.command.About"
                  label="About"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
