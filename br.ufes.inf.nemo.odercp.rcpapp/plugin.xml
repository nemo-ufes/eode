      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.DeleteProjectHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.DeleteProject"
            name="Delete Project">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKHumanResourceHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKHumanResource"
            name="Create Human Resource">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKActivityHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKActivity"
            name="Create Activity">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKArtefactHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKArtefact"
            name="Create Artefact">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKLifeCycleModelHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKLifeCycleModel"
            name="Create Life-Cycle Model">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKProcedureHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKProcedure"
            name="Create Procedure">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKProcessHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKProcess"
            name="Create Process">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKHardwareResourceHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKHardwareResource"
            name="Create Hardware Resource">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CreateKSoftwareResourceHandler"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.CreateKSoftwareResource"
            name="Create Software Resource">
      </command>
      <command
            defaultHandler="br.ufes.inf.nemo.odercp.rcpapp.Handlers.CallEditorODE"
            id="br.ufes.inf.nemo.odercp.rcpapp.command.ViewDataODE"
            name="Datas ODE">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="br.ufes.inf.nemo.odercp.rcpapp.pespective.PerspectiveODE"
            id="br.ufes.inf.nemo.odercp.rcpapp.perspective.PerspectiveODE"
            name="PerspectiveODE">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            class="br.ufes.inf.nemo.odercp.rcpapp.views.ViewDataODE"
            id="br.ufes.inf.nemo.odercp.rcpapp.views.ViewDataODE"
            name="Datas ODE"
            restorable="true">
      </view>
     
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="br.ufes.inf.nemo.odercp.rcpapp.editors.EditorODE"
            default="false"
            id="br.ufes.inf.nemo.odercp.rcpapp.editors.EditorODE"
            name="Datas ODE Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <sourceProvider
            provider="br.ufes.inf.nemo.odercp.rcpapp.sourceprovider.MenuState">
         <variable
               name="br.ufes.inf.nemo.odercp.rcpapp.sourceprovider.active"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
   </extension>


</plugin>
